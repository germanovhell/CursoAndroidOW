<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="safedev.holamundo.ComponentesActivity">

    <CheckBox
        android:id="@+id/checkBoxCondiciones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Acepto las condiciones"
        android:onClick="clickCheckBox"
        tools:layout_editor_absoluteX="145dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.093"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/checkBoxSexo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="clickCheckBox"
        android:text="Soy un Hombre" />
</LinearLayout>
